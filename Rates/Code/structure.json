            "pr.date_created",
            "pr.modified_by",
            "uu_8881.name AS modified_by_name",
            "pr.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Setup\\Countries\\Code\\Tables\\Countries",
                "alias": "sc",
                "name": "#__setup_countries",
                "on": "pr.country_id = sc.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "pr.created_by = uu.id"
            },
