    "query": {
        "from": {
            "table": "Payments\\Rates\\Code\\Tables\\Rates",
            "alias": "pr",
            "name": "#__payments_rates"
        },
        "select": [
            "pr.id",
            "pr.title",
            "pr.short_name",
            "pr.value",
            "pr.rule",
            "pr.type",
            "pr.effected_on",
