    "query": {
        "from": {
            "table": "Payments\\Rates\\Files\\Code\\Tables\\Files",
            "alias": "frf",
            "name": "#__payments_rates_files"
        },
        "select": [
            "frf.id",
            "frf.year",
            "frf.month",
            "frf.token",
            "frf.is_processed",
            "frf.type",
            "frf.max_limit",
            "frf.file",
            "mm.title AS file_title",
            "mm.file AS file_file",
            "frf.created_by",
            "uu.name AS created_by_name",
            "frf.date_created",
            "frf.modified_by",
            "uu_3095.name AS modified_by_name",
            "frf.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "frf.file = mm.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "frf.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_3095",
                "name": "#__users_users",
                "on": "frf.modified_by = uu_3095.id"
            }
        ]
    }
