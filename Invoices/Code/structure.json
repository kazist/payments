    "query": {
        "from": {
            "table": "Payments\\Invoices\\Code\\Tables\\Invoices",
            "alias": "pi",
            "name": "#__payments_invoices"
        },
        "select": [
            "pi.id",
            "pi.invoice_no",
            "pi.gateway_id",
            "pg.short_name AS gateway_id_short_name",
            "pi.user_id",
            "uu.name AS user_id_name",
            "uu.username AS user_id_username",
            "pi.item_id",
            "pi.payment_source",
            "pi.description",
            "pi.amount",
            "pi.quantity",
            "pi.completed",
            "pi.successful",
            "pi.is_canceled",
            "pi.expiry_date",
            "pi.created_by",
            "pi.date_created",
            "pi.modified_by",
            "pi.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Payments\\Gateways\\Code\\Tables\\Gateways",
                "alias": "pg",
                "name": "#__payments_gateways",
                "on": "pi.gateway_id = pg.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "pi.user_id = uu.id"
            }
        ]
    }
