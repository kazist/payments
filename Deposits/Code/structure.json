    "query": {
        "from": {
            "table": "Payments\\Deposits\\Code\\Tables\\Deposits",
            "alias": "pd",
            "name": "#__payments_deposits"
        },
        "select": [
            "pd.id",
            "pd.description",
            "pd.user_id",
            "uu.name AS user_id_name",
            "uu.username AS user_id_username",
            "pd.payment_id",
            "pp.receipt_no AS payment_id_receipt_no",
            "pp.gateway_id AS payment_id_gateway_id",
            "pd.completed",
            "pd.successful",
            "pd.amount",
            "pd.created_by",
            "uu_1993.name AS created_by_name",
            "pd.date_created",
            "pd.modified_by",
            "uu_8579.name AS modified_by_name",
            "pd.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "pd.user_id = uu.id"
            },
            {
                "table": "Payments\\Payments\\Code\\Tables\\Payments",
                "alias": "pp",
                "name": "#__payments_payments",
                "on": "pd.payment_id = pp.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_1993",
                "name": "#__users_users",
                "on": "pd.created_by = uu_1993.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_8579",
                "name": "#__users_users",
                "on": "pd.modified_by = uu_8579.id"
            }
        ]
    }
