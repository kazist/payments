    "query": {
        "from": {
            "table": "Payments\\Payments\\Items\\Code\\Tables\\Items",
            "alias": "ppi",
            "name": "#__payments_payments_items"
        },
        "select": [
            "ppi.id",
            "ppi.payment_id",
            "pp.receipt_no AS payment_id_receipt_no",
            "ppi.item_id",
            "ppi.user_id",
            "uu.name AS user_id_name",
            "ppi.payment_source",
            "ppi.description",
            "ppi.amount",
            "ppi.quantity",
            "ppi.unit_price",
            "ppi.created_by",
            "ppi.date_created",
            "ppi.modified_by",
            "ppi.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Payments\\Payments\\Code\\Tables\\Payments",
                "alias": "pp",
                "name": "#__payments_payments",
                "on": "ppi.payment_id = pp.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "ppi.user_id = uu.id"
            }
        ]
    }
