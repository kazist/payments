    "query": {
        "from": {
            "table": "Payments\\Payments\\Evripay\\Code\\Tables\\Evripay",
            "alias": "fpe",
            "name": "#__payments_payments_evripay"
        },
        "select": [
            "fpe.id",
            "fpe.payment_id",
            "fp.code AS payment_id_code",
            "fpe.card_status",
            "fpe.merchant_reference",
            "fpe.bank_reference",
            "fpe.transaction_data",
            "fpe.description",
            "fpe.created_by",
            "uu.name AS created_by_name",
            "fpe.date_created",
            "fpe.modified_by",
            "uu_7521.name AS modified_by_name",
            "fpe.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Payments\\Payments\\Code\\Tables\\Payments",
                "alias": "fp",
                "name": "#__payments_payments",
                "on": "fpe.payment_id = fp.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "fpe.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_7521",
                "name": "#__users_users",
                "on": "fpe.modified_by = uu_7521.id"
            }
        ]
    }
