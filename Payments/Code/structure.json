    "query": {
        "from": {
            "table": "Finance\\Payments\\Code\\Tables\\Payments",
            "alias": "fp",
            "name": "#__finance_payments"
        },
        "select": [
            "fp.id",
            "fp.gateway_id",
            "fg.short_name AS gateway_id_short_name",
            "fp.user_id",
            "uu.name AS user_id_name",
            "uu.username AS user_id_username",
            "fp.subset_id",
            "ss.title AS subset_id_title",
            "fp.item_id",
            "fp.receipt_no",
            "fp.code",
            "fp.type",
            "fp.description",
            "fp.amount",
            "fp.amount_required",
            "fp.amount_paid",
            "fp.quantity",
            "fp.params",
            "fp.deductions",
            "fp.completed",
            "fp.successful",
            "fp.is_canceled",
            "fp.created_by",
            "uu_8817.name AS created_by_name",
            "fp.date_created",
            "fp.modified_by",
            "uu_3563.name AS modified_by_name",
            "fp.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Finance\\Gateways\\Code\\Tables\\Gateways",
                "alias": "fg",
                "name": "#__finance_gateways",
                "on": "fp.gateway_id = fg.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "fp.user_id = uu.id"
            },
            {
                "table": "System\\Subsets\\Code\\Tables\\Subsets",
                "alias": "ss",
                "name": "#__system_subsets",
                "on": "fp.subset_id = ss.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_8817",
                "name": "#__users_users",
                "on": "fp.created_by = uu_8817.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_3563",
                "name": "#__users_users",
                "on": "fp.modified_by = uu_3563.id"
            }
        ]
    }
