            "fpi.created_by",
            "uu.name AS created_by_name",
            "fpi.date_created",
            "fpi.modified_by",
            "uu_4863.name AS modified_by_name",
            "fpi.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Payments\\Payments\\Code\\Tables\\Payments",
                "alias": "fp",
                "name": "#__payments_payments",
                "on": "fpi.payment_id = fp.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "fpi.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_4863",
                "name": "#__users_users",
                "on": "fpi.modified_by = uu_4863.id"
