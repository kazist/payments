    "query": {
        "from": {
            "table": "Payments\\Payments\\Ipay\\Code\\Tables\\Ipay",
            "alias": "fpi",
            "name": "#__payments_payments_ipay"
        },
        "select": [
            "fpi.id",
            "fpi.payment_id",
            "fp.code AS payment_id_code",
            "fpi.item_id",
            "fpi.status",
