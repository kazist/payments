    "query": {
        "from": {
            "table": "Payments\\Gateways\\Transferrates\\Code\\Tables\\Transferrates",
            "alias": "pgt",
            "name": "#__payments_gateways_transferrates"
        },
        "select": [
            "pgt.id",
            "pgt.gateway_id",
            "pg.long_name AS gateway_id_long_name",
            "pgt.rate_id",
            "fr.title AS rate_id_title",
            "pgt.created_by",
            "uu.name AS created_by_name",
            "pgt.date_created",
            "pgt.modified_by",
            "uu_7891.name AS modified_by_name",
            "pgt.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Payments\\Gateways\\Code\\Tables\\Gateways",
                "alias": "pg",
                "name": "#__payments_gateways",
                "on": "pgt.gateway_id = pg.id"
            },
            {
                "table": "Payments\\Rates\\Code\\Tables\\Rates",
                "alias": "fr",
                "name": "#__payments_rates",
                "on": "pgt.rate_id = fr.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "pgt.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_7891",
                "name": "#__users_users",
                "on": "pgt.modified_by = uu_7891.id"
            }
        ]
    }
