    "query": {
        "from": {
            "table": "Payments\\Gateways\\Allowedin\\Code\\Tables\\Allowedin",
            "alias": "pga",
            "name": "#__payments_gateways_allowedin"
        },
        "select": [
            "pga.id",
            "pga.country_id",
            "sc.name AS country_id_name",
            "pga.gateway_id",
            "pg.long_name AS gateway_id_long_name",
            "pga.created_by",
            "uu.name AS created_by_name",
            "pga.date_created",
            "pga.modified_by",
            "uu_4802.name AS modified_by_name",
            "pga.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Setup\\Countries\\Code\\Tables\\Countries",
                "alias": "sc",
                "name": "#__setup_countries",
                "on": "pga.country_id = sc.id"
            },
            {
                "table": "Payments\\Gateways\\Code\\Tables\\Gateways",
                "alias": "pg",
                "name": "#__payments_gateways",
                "on": "pga.gateway_id = pg.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "pga.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_4802",
                "name": "#__users_users",
                "on": "pga.modified_by = uu_4802.id"
            }
        ]
    }
