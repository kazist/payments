            "pc.date_created",
            "pc.modified_by",
            "uu_871.name AS modified_by_name",
            "pc.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "pc.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_871",
                "name": "#__users_users",
                "on": "pc.modified_by = uu_871.id"
            }
        ]
    }
