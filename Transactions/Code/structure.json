    "query": {
        "from": {
            "table": "Payments\\Transactions\\Code\\Tables\\Transactions",
            "alias": "ft",
            "name": "#__payments_transactions"
        },
        "select": [
            "ft.id",
            "ft.user_id",
            "uu.name AS user_id_name",
            "ft.item_id",
            "ft.payment_id",
            "fp.code AS payment_id_code",
            "ft.source",
            "ft.rate_id",
            "fr.title AS rate_id_title",
            "ft.description",
            "ft.credit",
            "ft.debit",
            "ft.type",
            "ft.level",
            "ft.is_capped",
            "ft.behalf_user_id",
            "uu_9079.name AS behalf_user_id_name",
            "ft.token",
            "ft.is_processed",
            "ft.created_by",
            "uu_5281.name AS created_by_name",
            "ft.date_created",
            "ft.modified_by",
            "uu_9119.name AS modified_by_name",
            "ft.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "ft.user_id = uu.id"
            },
            {
                "table": "Payments\\Payments\\Code\\Tables\\Payments",
                "alias": "fp",
                "name": "#__payments_payments",
                "on": "ft.payment_id = fp.id"
            },
            {
                "table": "Payments\\Rates\\Code\\Tables\\Rates",
                "alias": "fr",
                "name": "#__payments_rates",
                "on": "ft.rate_id = fr.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_9079",
                "name": "#__users_users",
                "on": "ft.behalf_user_id = uu_9079.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_5281",
                "name": "#__users_users",
                "on": "ft.created_by = uu_5281.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_9119",
                "name": "#__users_users",
                "on": "ft.modified_by = uu_9119.id"
            }
        ]
    }
